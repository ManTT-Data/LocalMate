# ===========================================
# Da Nang Tourism RAG API - Environment Variables
# ===========================================
# Copy this file to .env and fill in your values

# ------------------------------------------
# NEO4J (Graph Database)
# ------------------------------------------
NEO4J_URI=neo4j+s://xxxxx.databases.neo4j.io
NEO4J_USERNAME=neo4j
NEO4J_PASSWORD=your-neo4j-password

# ------------------------------------------
# SUPABASE (Vector Database)
# ------------------------------------------
# For direct PostgreSQL connection
SUPABASE_DB_PASSWORD=your-supabase-db-password
SUPABASE_DB_HOST=aws-1-ap-northeast-2.pooler.supabase.com
SUPABASE_DB_USER=postgres.your-project-id

# For Supabase client (optional, only for upload)
SUPABASE_URL=https://xxxxx.supabase.co
SUPABASE_KEY=your-supabase-anon-key

# ------------------------------------------
# GOOGLE AI (Embeddings & LLM)
# ------------------------------------------
GOOGLE_API_KEY=your-google-api-key
